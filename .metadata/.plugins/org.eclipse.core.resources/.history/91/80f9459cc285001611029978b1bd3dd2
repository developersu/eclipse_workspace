import pycurl
import re
import array
import string
from io import BytesIO

buf = BytesIO()
c = pycurl.Curl()
c.setopt(c.URL, 'http://ibash.org.ru/quote.php?id=17395')
c.setopt(c.WRITEDATA, buf)
c.perform()
c.close()

line = re.sub('<[^>]*>','',buf.getvalue().decode('cp1251'))
line1 = line.split('\n')
#line1.toString()
